{
  "name": "clanker-telegram-bot",
  "version": "1.0.0",
  "description": "Telegram bot for deploying Clanker tokens",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "format": "biome format --write ."
  },
  "keywords": [
    "telegram",
    "bot",
    "clanker",
    "token",
    "deployment"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "node-telegram-bot-api": "^0.66.0",
    "dotenv": "^16.4.5",
    "pg": "^8.11.3",
    "ioredis": "^5.3.2",
    "zod": "^3.23.8",
    "viem": "^2.21.45",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/node-telegram-bot-api": "^0.64.7",
    "@types/pg": "^8.11.0",
    "@biomejs/biome": "^1.8.3",
    "tsup": "^8.0.1",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5"
  }
}

